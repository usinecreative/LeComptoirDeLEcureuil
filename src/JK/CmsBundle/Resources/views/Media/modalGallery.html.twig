<ul class="media-list">
    {% for media in mediaList %}
        <li class="media">
            <a href="#" class="media-link" data-id="{{ media.id }}">
                <img src="{{ cms_media_path(media, true, true, 'media_thumbnail') }}"
                     alt="{{ media.description }}"/>
                <br/>{{ media.name }}
            </a>
        </li>
    {% endfor %}
</ul>
