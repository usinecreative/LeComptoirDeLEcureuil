{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "require": {
        "php": ">=7.0",
        "ext-gd": "*",
        "blueimp/jquery-file-upload": "^9.18",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "dunglas/action-bundle": "^0.4.1",
        "eko/feedbundle": "^1.2",
        "hwi/oauth-bundle": "^0.5.3",
        "incenteev/composer-parameter-handler": "^2.1",
        "johnkrovitch/sam-bundle": "dev-release/v0.1.1",
        "knplabs/knp-gaufrette-bundle": "^0.4.0",
        "liip/imagine-bundle": "^1.8",
        "google/recaptcha": "^1.1",
        "guzzlehttp/guzzle": "^6.2",
        "oneup/uploader-bundle": "^1.7",
        "tinymce/tinymce": "^4.6",
        "twig/extensions": "^1.5",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "sensio/generator-bundle": "^3.1",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "~3.0",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/symfony": "~3.2",

        "twbs/bootstrap": "4.0.0",
        "components/font-awesome": "~4.5",
        "components/jquery": "~3.0",
        "components/jqueryui": "~1.0",
        "egeloen/ckeditor-bundle": "^4.0",
        "gedmo/doctrine-extensions": "^2.4",
        "johnkrovitch/configuration": "^1.0",
        "knplabs/knp-menu-bundle": "^2.1",
        "lag/array-utils": "^0.1",
        "stfalcon/tinymce-bundle": "~2.0",
        "symfony/monolog-bundle": "~3.0",
        "white-october/pagerfanta-bundle": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "symfony/web-server-bundle": "^3.4",
        "hautelook/alice-bundle": "^2.0",
        "doctrine/data-fixtures": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "component-dir": "web/assets",
        "minimum-stability": "dev"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "symlink",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
