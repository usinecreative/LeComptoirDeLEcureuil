    {% block latestArticles %}{% endblock %}

    <div class="row margin-row">
        <footer>
            <p>Site sous le copyright des noisettes - Le Comptoir De L'Ecureuil 2015<br/></p>
            <ul>
                <li><a href="{{ path('lecomptoir.contact') }}"><i class="fa fa-envelope"></i>&nbsp;Contact</a></li>
                <li><a href="{{ path('lecomptoir.sitemap') }}"><i class="fa fa-sitemap"></i>&nbsp;Plan du site</a></li>
                <li><a href="#"><i class="fa fa-briefcase"></i>&nbsp;Partenariats</a></li>
                <li><a href="#"><i class="fa fa-rss"></i>&nbsp;Rss</a></li>
            </ul>
        </footer>
    </div>
</div>
{% endblock %}

<script src="/bundles/lagadmin/js/jquery/jquery.min.js"></script>
<script src="/bundles/lagadmin/js/bootstrap.min.js"></script>
</body>
